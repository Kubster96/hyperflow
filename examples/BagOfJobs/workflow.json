{
    "name": "BagOfJobsExample",
    "processes": [ {
        "name": "BojExecutor",
	"type": "dataflow",
	"firingLimit": 1,
        "function": "bojK8sCommand",
	"config": {
		"executor": {
			"image": "hyperflowwms/soykb-workflow-worker",
			"executable": "echo",
			"args": ["Hello", "World"],
			"stdout": "test.stdout"
		}
	},
        "ins": [ "jobSetsFile" ],
        "outs": [ ]
    } ],
    "signals": [ {
        "name": "jobSetsFile",
        "data": [ "job_sets.json", "workload.json" ]
    } ],
    "ins": [ "jobSetsFile" ],
    "outs": [ ]
}
